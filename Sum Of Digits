class SumOfDigits 
{
    public static void main(String[] args) 
    {
        
        System.out.println(sumDigits(1));
    }
    
    public static int sumDigits(int number)
    {
        int sum=0;
        if(number<10)
        {
            return -1;
        }
        while(number>0)
        {
           int b=number%10;
           sum= sum+b;
           number=number/10; 
        }
        return sum;
    }
}
